{
  "Serilog": {
    "Using": ["Serilog.Sinks.Console"],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": ["FromLogContext", "WithThreadId", "WithMachineName"]
  },
  "Kestrel": {
    "EndPoints": {
      "Grpc": {
        "Url": "http://*:7351",
        "Protocols": "Http2"
      },
      "Http": {
        "Url": "http://*:7350",
        "Protocols": "Http1AndHttp2"
      },
      "Https": {
        "Url": "http://*:7355",
        "Protocols": "Http1AndHttp2"
      }
    }
  }
}
